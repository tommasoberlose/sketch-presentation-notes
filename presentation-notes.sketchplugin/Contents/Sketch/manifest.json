{
  "name": "Presentation Notes",
  "description": "Plugin that add presentation notes to artboards.",
  "author": "Tommaso Berlose",
  "homepage": "https://github.com/tommasoberlose/sketch-presentation-notes",
  "version": "1.0",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Add notes to selected artboards",
      "identifier": "add-notes",
      "shortcut": "ctrl shift n",
      "script": "add-notes.js"
    },
    {
      "name": "Remove notes form selected artboards",
      "identifier": "remove-notes",
      "shortcut": "ctrl shift alt n",
      "script": "remove-notes.js"
    },
    {
      "name": "Toggle all notes",
      "identifier": "toggle-notes",
      "shortcut": "ctrl shift t",
      "script": "toggle-notes.js"
    }
  ],
  "menu": {
    "title": "Presentation Notes",
    "items": [
      "add-notes",
      "remove-notes",
      "toggle-notes"
    ]
  },
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml"
}